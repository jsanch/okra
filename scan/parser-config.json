{
	"mid_parsers": {
		"total": {
			"ere": "^total( due)?$",
			"string": "total"
		},
		"subtotal": {
			"ere": "^sub[ -]?(total)$",
			"string": "subtotal"
		},
		"balance_due": {
			"ere": "^balance( due)?)$",
			"string": "balance_due"
		},
		"tax": {
			"ere": "^((sales)|(meals)|(total))?( tax)?)$",
			"string": "tax"
		}
	},
	"pre_parsers": {
		"test": {
			"ere": "(sub)[ -]?(total)",
			"string": "test"
		},
		"end_of_line_price": {
			"ere": "\\$?[0-9]+[\\.,][0-9]{2}$",
			"string": "end_of_line"
		}
	}
}